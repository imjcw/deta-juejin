# Spacefile Docs: https://go.deta.dev/docs/spacefile/v0
v: 0
micros:
  - name: juejinSign
    src: .
    engine: custom
    primary: true
    commands:
      - go build -o juejinSign
    include:
      - juejinSign
    run: ./juejinSign
    presets:
      env:
        - name: SIGN_URL
    dev: go run main.go
    actions:
      - id: "signer"
        name: "signer"
        description: "Juejin signer."
        trigger: "schedule"
        default_interval: "0/17 * * * *"